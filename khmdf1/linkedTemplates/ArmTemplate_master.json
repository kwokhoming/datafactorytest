{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "khmdf1"
		},
		"ADLS_lego_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_lego'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"LegoBlobLog_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LegoBlobLog'"
		},
		"ADLS_lego_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://khmdbdatalakestorageacc.dfs.core.windows.net"
		},
		"HttpLegoServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "khmdf1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_lego_accountKey": {
						"value": "[parameters('ADLS_lego_accountKey')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"LegoBlobLog_connectionString": {
						"value": "[parameters('LegoBlobLog_connectionString')]"
					},
					"ADLS_lego_properties_typeProperties_url": {
						"value": "[parameters('ADLS_lego_properties_typeProperties_url')]"
					},
					"HttpLegoServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpLegoServer1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}